.containers {
  width: 100%;
  height: 100%;
}

.city_title {
  padding: 30rpx 10rpx 5rpx;
  color: gray;
  font-size: 30rpx;
}

.content {
  background-color: #fff;
  font-size: 30rpx;
}

.city_name {
  padding: 20rpx;
  border-bottom: 1rpx solid gainsboro;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.city_name image {
  display: flex;
  width: 45rpx;
  height: 30rpx;
}
